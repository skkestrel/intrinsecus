<Window x:Name="SelectionDialogueWindow" x:Class="EhT.Intrinsecus.SelectionDialogue"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Selection" Height="320" Width="203">
	<Grid x:Name="MainGrid">
		<Grid.RowDefinitions>
			<RowDefinition/>
			<RowDefinition/>
			<RowDefinition/>
			<RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
		<Button x:Name="SquatsButton" Content="Squats" Margin="10" Click="SquatsButton_Click" Grid.Row="1"/>
		<Button x:Name="DeadliftsButton" Content="Deadlifts" Margin="10" Click="DeadliftsButton_Click"/>
		<Button x:Name="ShoulderPressesButton" Content="Shoulder Press" Margin="10" Click="ShoulderPressesButton_Click" Grid.Row="2"/>
		<Button x:Name="SplitLegLungesButton" Content="Split leg lunges" Margin="10" Click="SplitLegLungesButton_Click" Grid.Row="3"/>
        <Button x:Name="JumpingJacksButton" Content="Jumping Jacks" Margin="10" Click="JumpingJacksButton_Click" Grid.Row="4"/>
        <Button x:Name="VerticalJumpButton" Content="Vertical Jump Test" Margin="10" Click="VerticalJumpButton_Click" Grid.Row="5"/>
        <Button x:Name="LateralFlyButton" Content="Lateral Fly Test" Margin="10" Click="LateralFlyButton_Click" Grid.Row="6"/>


    </Grid>
</Window>
